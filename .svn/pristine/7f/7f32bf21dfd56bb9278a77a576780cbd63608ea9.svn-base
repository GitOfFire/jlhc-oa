package com.jlhc.common.exception;

import org.apache.shiro.authc.AuthenticationException;

public class NotVertifyException extends AuthenticationException {
    public NotVertifyException() {
    }

    public NotVertifyException(String message) {
        super(message);
    }

    public NotVertifyException(Throwable cause) {
        super(cause);
    }

    public NotVertifyException(String message, Throwable cause) {
        super(message, cause);
    }
}
