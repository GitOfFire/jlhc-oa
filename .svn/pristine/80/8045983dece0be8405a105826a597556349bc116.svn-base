  <!--通过用户id查询对应的组织机构-->
  <select id="selectOrganizationByUserId" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    SELECT
      org.org_id AS org_id,
      org.org_parent_id AS org_parent_id,
      org.org_name AS org_name,
      org.org_createdtime AS org_createdtime,
      org.org_description AS org_description
    FROM
      jl_organization org,jl_user_org uo
    WHERE
      uo.user_id= #{userId,jdbcType=INTEGER}
      AND
      uo.org_id = org.org_id;

  </select>
